    "\u0d9a\u0ddc\u0dc5\u0db9 \u0d9a\u0ddc\u0da7\u0dc3\u0dca \u0dc0\u0dd9\u0dc5\u0dd9\u0db3\u0db4\u0ddc\u0dc5 \u0dc3\u0dd2\u0dba\u0dbd\u0dd4 \u0d9a\u0ddc\u0da7\u0dc3\u0dca \u0db8\u0dd2\u0dbd \u0daf\u0dbb\u0dca\u0dc1\u0d9a\u0dba \u0d89\u0dad\u0dd2\u0dc4\u0dcf\u0dc3\u0dba\u0dda \u0db4\u0dc5\u0db8\u0dd4\u0dc0\u0dbb\u0da7 \u0d85\u0daf (2) \u0d92\u0d9a\u0d9a 16,000 \u0dc3\u0dd3\u0db8\u0dcf\u0dc0 \u0d89\u0d9a\u0dca\u0db8\u0dc0\u0dcf \u0d9c\u0dd2\u0dba\u0dda\u0dba.",
    "2024 \u0dc0\u0dc3\u0dbb\u0dda\u0daf\u0dd3 \u0daf \u0d9a\u0ddc\u0dc5\u0db9 \u0d9a\u0ddc\u0da7\u0dc3\u0dca \u0dc0\u0dd9\u0dc5\u0dd9\u0db3\u0db4\u0ddc\u0dc5 \u0dc3\u0dd2\u0dba\u0dbd\u0dd4 \u0d9a\u0ddc\u0da7\u0dc3\u0dca \u0db8\u0dd2\u0dbd \u0daf\u0dbb\u0dca\u0dc1\u0d9a\u0dba 49.66%\u0d9a \u0dc3\u0dd4\u0dc0\u0dd2\u0dc1\u0dda\u0dc2\u0dd3 \u0dc0\u0dbb\u0dca\u0db0\u0db1\u0dba\u0d9a\u0dca \u0d85\u0dad\u0dca\u0db4\u0dad\u0dca \u0d9a\u0dbb\u0d9c\u0dd0\u0db1\u0dd3\u0db8\u0da7 \u0dc3\u0db8\u0dad\u0dca \u0dc0\u0dd2\u0dba.",
    "\u0d89\u0db1\u0dca \u0d9c\u0dd9\u0dc0\u0dd4\u0dab\u0dd4 \u0db8\u0dcf\u0dc3 3\u0d9a \u0d9a\u0dcf\u0dbd\u0dba \u0dad\u0dd4\u0dc5 \u0daf\u0dd2\u0d9c\u0dd2\u0db1\u0dca \u0daf\u0dd2\u0d9c\u0da7\u0db8 \u0dc0\u0dbb\u0dca\u0db0\u0db1\u0dba\u0d9a\u0dca \u0db4\u0dd9\u0db1\u0dca\u0db1\u0dd4\u0db8\u0dca \u0d9a\u0dc5\u0dda\u0dba.",
    "\u0db1\u0ddc\u0dc0\u0dd0\u0db8\u0dca\u0db6\u0dbb\u0dca 13 \u0dc0\u0db1\u0dd2\u0daf\u0dcf \u0dc3\u0dd2\u0dba\u0dbd\u0dd4 \u0d9a\u0ddc\u0da7\u0dc3\u0dca \u0db8\u0dd2\u0dbd \u0daf\u0dbb\u0dca\u0dc1\u0d9a\u0dba \u0db4\u0dc5\u0db8\u0dd4\u0dc0\u0dbb\u0da7 \u0d92\u0d9a\u0d9a 13,000 \u0db4\u0dc3\u0dd4\u0d9a\u0dc5 \u0d85\u0dc0\u0dc3\u0dca\u0dae\u0dcf\u0dc0\u0dd9\u0db1\u0dca \u0db4\u0dc3\u0dd4\u0dc0 \u0d85\u0d9b\u0dab\u0dca\u0da9\u0dc0 \u0d91\u0db8 \u0dc0\u0dcf\u0dbb\u0dca\u0dad\u0dcf\u0dc0 \u0d85\u0dbd\u0dd4\u0dad\u0dca \u0dc0\u0dd2\u0dba.",
    "\u0d92 \u0d85\u0db1\u0dd4\u0dc0 \u0daf\u0dd9\u0dc3\u0dd0\u0db8\u0dca\u0db6\u0dbb\u0dca 12\u0dc0\u0db1\u0daf\u0dcf \u0d92\u0d9a\u0d9a 14,000\u0d9a\u0dca \u0dc0\u0dcf\u0dbb\u0dca\u0dad\u0dcf \u0dc0\u0dd6 \u0d85\u0dad\u0dbb, \u0daf\u0dd9\u0dc3\u0dd0\u0db8\u0dca\u0db6\u0dbb\u0dca 23 \u0dc0\u0db1\u0daf\u0dcf \u0d92\u0d9a\u0d9a 15,000 \u0dc3\u0dd3\u0db8\u0dcf\u0dc0 \u0db4\u0dc3\u0dd4 \u0d9a\u0dc5\u0dda\u0dba.",
    "\u0d91\u0db8 \u0daf\u0dbb\u0dca\u0dc1\u0d9a\u0dba \u0d85\u0daf \u0d92\u0d9a\u0d9a 16,000 \u0dc3\u0dd3\u0db8\u0dcf\u0dc0 \u0d89\u0d9a\u0dca\u0db8\u0dc0\u0dcf \u0d9c\u0dd2\u0dba\u0dda \u0d89\u0dad\u0dd2\u0dc4\u0dcf\u0dc3\u0dba\u0dda \u0db4\u0dc5\u0db8\u0dd4 \u0dc0\u0dbb\u0da7\u0dba.",
