    "2019 \u0dc0\u0dc3\u0dbb\u0dda \u0db4\u0dd0\u0dc0\u0dad\u0dd2 \u0da2\u0db1\u0dcf\u0db0\u0dd2\u0db4\u0dad\u0dd2\u0dc0\u0dbb\u0dab\u0dba\u0dd9\u0db1\u0dca \u0d9c\u0ddd\u0da8\u0dcf\u0db7\u0dba \u0dbb\u0dcf\u0da2\u0db4\u0d9a\u0dca\u0dc2 \u0da2\u0dba \u0d9c\u0dd0\u0db1\u0dd3\u0db8\u0dd9\u0db1\u0dca \u0db4\u0dc3\u0dd4\u0dc0 \u0dc3\u0dc4 2020 \u0dc0\u0dc3\u0dbb\u0dda \u0db8\u0dc4 \u0db8\u0dd0\u0dad\u0dd2\u0dc0\u0dbb\u0dab\u0dba\u0dd9\u0db1\u0dca \u0dc1\u0dca\u200d\u0dbb\u0dd3 \u0dbd\u0d82\u0d9a\u0dcf \u0db4\u0ddc\u0daf\u0dd4\u0da2\u0db1 \u0db4\u0dd9\u0dbb\u0db8\u0dd4\u0dab \u0da2\u0dba \u0d9c\u0dd0\u0db1\u0dd3\u0db8\u0dd9\u0db1\u0dca \u0db4\u0dc3\u0dd4\u0dc0 \u0d9a\u0dd9\u0da7\u0dd2 \u0d9a\u0dbd\u0d9a\u0dd2\u0db1\u0dca \u0dbb\u0da7 \u0dad\u0dd4\u0dc5 \u0d87\u0dad\u0dd2 \u0dc0\u0dd6 \u0d86\u0dbb\u0dca\u0dae\u0dd2\u0d9a \u0d85\u0dbb\u0dca\u0db6\u0dd4\u0daf\u0dba \u0dc4\u0dda\u0dad\u0dd4\u0dc0\u0dd9\u0db1\u0dca \u0da2\u0db1 \u0d85\u0dbb\u0d9c\u0dbd\u0dba\u0d9a\u0dca \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab\u0dba \u0dc0\u0dd2\u0dba.",
    "\u0d91\u0db8 \u0dc0\u0dd2\u0dbb\u0ddd\u0db0\u0dba \u0d85\u0dad\u0dbb\u0dad\u0dd4\u0dbb \u0dbb\u0db1\u0dd2\u0dbd\u0dca \u0dc0\u0dd2\u0d9a\u0dca\u200d\u0dbb\u0db8\u0dc3\u0dd2\u0d82\u0dc4 \u0da2\u0db1\u0dcf\u0db0\u0dd2\u0db4\u0dad\u0dd2 \u0db0\u0dd6\u0dbb\u0dba\u0da7 \u0db4\u0dad\u0dca\u0dc0\u0dd6 \u0d85\u0dad\u0dbb, \u0da2\u0dcf\u0dad\u0dd2\u0d9a \u0da2\u0db1 \u0db6\u0dbd\u0dc0\u0dda\u0d9c\u0dba \u0d9a\u0dd9\u0dbb\u0dd9\u0dc4\u0dd2 \u0da2\u0db1\u0dad\u0dcf \u0d85\u0dc0\u0db0\u0dcf\u0db1\u0dba \u0dba\u0ddc\u0db8\u0dd4 \u0dc0\u0db1\u0dca\u0db1\u0da7 \u0dc0\u0dd2\u0dba.",
    "\u0dc1\u0dca\u200d\u0dbb\u0dd3 \u0dbd\u0d82\u0d9a\u0dcf\u0dc0\u0dda 9 \u0dc0\u0db1 \u0da2\u0db1\u0dcf\u0db0\u0dd2\u0db4\u0dad\u0dd2\u0dc0\u0dbb\u0dab\u0dba\u0dd9\u0db1\u0dca \u0da2\u0dcf\u0dad\u0dd2\u0d9a \u0da2\u0db1 \u0db6\u0dbd\u0dc0\u0dda\u0d9c\u0dba\u0dda \u0d85\u0db4\u0dda\u0d9a\u0dca\u0dc2\u0d9a \u0d85\u0db1\u0dd4\u0dbb \u0d9a\u0dd4\u0db8\u0dcf\u0dbb \u0daf\u0dd2\u0dc3\u0dcf\u0db1\u0dcf\u0dba\u0d9a\u0da7 \u0da2\u0dba \u0dc4\u0dd2\u0db8\u0dd2 \u0dc0\u0dd6\u0dba\u0dda, \u0d91\u0dc0\u0dd0\u0db1\u0dd2 \u0db4\u0dc3\u0dd4\u0db6\u0dd2\u0db8\u0d9a\u0dca \u0dad\u0dd4\u0dc5 \u0dba.",
    "\u0db4\u0dc3\u0dd4\u0d9c\u0dd2\u0dba \u0db8\u0dc4 \u0db8\u0dd0\u0dad\u0dd2\u0dc0\u0dbb\u0dab\u0dba\u0dd9\u0db1\u0dca \u0da2\u0dcf\u0dad\u0dd2\u0d9a \u0da2\u0db1 \u0db6\u0dbd\u0dc0\u0dda\u0d9c\u0dba\u0da7 \u0dc4\u0dd2\u0db8\u0dd2 \u0dc0\u0dd6\u0dba\u0dda, \u0db4\u0dcf\u0dbb\u0dca\u0dbd\u0dd2\u0db8\u0dda\u0db1\u0dca\u0dad\u0dd4\u0dc0\u0dda \u0d86\u0dc3\u0db1 \u0dad\u0dd4\u0db1\u0d9a\u0dca \u0db4\u0db8\u0dab\u0dd2.",
    "\u0da2\u0db1\u0dad\u0dcf \u0d85\u0dbb\u0d9c\u0dbd\u0dba\u0dda\u0daf\u0dd3 \u0db8\u0dd9\u0db1\u0dca \u0db8 \u0d8a\u0da7 \u0db4\u0dd9\u0dbb \u0daf \u0dc1\u0dca\u200d\u0dbb\u0dd3 \u0dbd\u0dcf\u0d82\u0d9a\u0dd2\u0d9a \u0da2\u0db1\u0dad\u0dcf\u0dc0\u0d9c\u0dd9\u0db1\u0dca \u0db6\u0dc4\u0dd4\u0dad\u0dbb\u0dba\u0d9a\u0dca \u0d85\u0dad\u0dbb \u0db4\u0dd0\u0dc0\u0dad\u0dd2 \u0db8\u0dad\u0dba\u0d9a\u0dca \u0dc0\u0dd6\u0dba\u0dda, \u0db4\u0dcf\u0dbb\u0dca\u0dbd\u0dd2\u0db8\u0dda\u0db1\u0dca\u0dad\u0dd4\u0dc0\u0dda 225 \u0db8 \u0d91\u0db4\u0dcf! \u0dba\u0db1\u0dca\u0db1 \u0dba.",
    "\u0d8a\u0da7 \u0d85\u0db8\u0dad\u0dbb\u0dc0, \u0db4\u0dca\u200d\u0dbb\u0db7\u0dd6 \u0db4\u0dd0\u0dc5\u0dd0\u0db1\u0dca\u0dad\u0dd2\u0dba \u0dc3\u0dc4 \u0db8\u0dc4\u0dbd\u0dd4 \u0daf\u0dda\u0dc1\u0db4\u0dcf\u0dbd\u0d9a\u0dba\u0dd2\u0db1\u0dca \u0daf \u0da2\u0db1\u0dad\u0dcf\u0dc0 \u0db4\u0dca\u200d\u0dbb\u0dad\u0dd2\u0d9a\u0dca\u0dc2\u0dda\u0db4 \u0d9a\u0dbb \u0dc3\u0dd2\u0da7\u0dd2\u0dba\u0dc4.",
    "\u0db8\u0dc4 \u0db8\u0dd0\u0dad\u0dd2\u0dc0\u0dbb\u0dab\u0dba \u0d9a\u0dd0\u0db3\u0dc0\u0dd6 \u0d85\u0dc0\u0dc3\u0dca\u0dae\u0dcf\u0dc0\u0dda\u0daf\u0dd3 \u0db8 \u0d91\u0db8 \u0d89\u0dbd\u0dca\u0dbd\u0dd3\u0db8\u0dca\u0dc0\u0dbd\u0dd2\u0db1\u0dca \u0d85\u0da9\u0d9a\u0dca \u0d89\u0dc2\u0dca\u0da7 \u0dc0\u0dd6\u0dba\u0dda, \u0dc4\u0dd2\u0da7\u0db4\u0dd4 \u0db8\u0db1\u0dca\u0dad\u0dca\u200d\u0dbb\u0dd3\u0dc0\u0dbb\u0dd4\u0db1\u0dca \u0dbb\u0dd0\u0dc3\u0d9a\u0dca \u0db8\u0dd9\u0dc0\u0dbb \u0db8\u0dc4 \u0db8\u0dd0\u0dad\u0dd2\u0dc0\u0dbb\u0dab\u0dba\u0da7 \u0dad\u0dbb\u0d9c \u0db1\u0ddc\u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 \u0dad\u0dd3\u0dbb\u0dab\u0dba \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0dad\u0dca \u0dc3\u0db8\u0d9c \u0dba.",
    "\u0d8a\u0da7 \u0d85\u0db8\u0dad\u0dbb\u0dc0, \u0da2\u0dcf\u0dad\u0dd2\u0d9a \u0da2\u0db1 \u0db6\u0dbd\u0dc0\u0dda\u0d9c\u0dba\u0dda \u0da2\u0dba\u0d9c\u0dca\u200d\u0dbb\u0dc4\u0dab\u0dba\u0dad\u0dca \u0dc3\u0db8\u0d9c \u0db4\u0dcf\u0dbb\u0dca\u0dbd\u0dd2\u0db8\u0dda\u0db1\u0dca\u0dad\u0dd4\u0dc0\u0dda \u0d86\u0dc3\u0db1 \u0dc4\u0dd2\u0db8\u0dd2 \u0d9a\u0dbb \u0d9c\u0dad\u0dca \u0d85\u0dad\u0dd2\u0dc1\u0dba \u0db6\u0dc4\u0dd4\u0dad\u0dbb\u0dba \u0db1\u0dc0\u0d9a\u0dba\u0dd2\u0db1\u0dca \u0dc3\u0dc4 \u0dad\u0dbb\u0dd4\u0dab\u0dba\u0dd2\u0db1\u0dca \u0dc0\u0dd3\u0db8 \u0daf \u0da2\u0db1\u0dad\u0dcf \u0d85\u0db7\u0dd2\u0dbd\u0dcf\u0dc2\u0dba \u0d89\u0dc2\u0dca\u0da7 \u0dc0\u0dd3\u0db8\u0d9a\u0dd2.",
    "\u0dc3\u0db8\u0d9c\u0dd2 \u0da2\u0db1 \u0db6\u0dbd\u0dc0\u0dda\u0d9c\u0dba \u0db4\u0dbb\u0dcf\u0da2\u0dba\u0dad\u0dca \u0dc3\u0db8\u0d9c \u0dc3\u0da2\u0dd2\u0dad\u0dca\u0d9c\u0dda \u0daf\u0dda\u0dc1\u0db4\u0dcf\u0dbd\u0db1 \u0d9c\u0db8\u0db1 \u0d85\u0dc0\u0dc3\u0db1\u0dca \u0dc0\u0dd9\u0dba\u0dd2 \u0daf?",
    "\u0d9c\u0ddd\u0dbd\u0dd3\u0dba \u0dc0\u0db1\u0da2\u0dd3\u0dc0\u0dd3 \u0da2\u0dcf\u0dc0\u0dcf\u0dbb\u0db8\u0dda \u0d9a\u0dda\u0db1\u0dca\u0daf\u0dca\u200d\u0dbb\u0dc3\u0dca\u0dae\u0dcf\u0db1\u0dba\u0d9a\u0dca \u0db6\u0dc0\u0da7 \u0dc1\u0dca\u200d\u0dbb\u0dd3 \u0dbd\u0d82\u0d9a\u0dcf\u0dc0 \u0db4\u0dad\u0dca \u0dc0\u0dd6\u0dba\u0dda \u0d9a\u0dd9\u0dc3\u0dda \u0daf?"
  ]
}